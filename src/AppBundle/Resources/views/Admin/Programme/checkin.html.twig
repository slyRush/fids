{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}
{% block field %}
    {% set status = object.checkIn %}
    {% if status == "cloture" %}
        Enregistrement clôturée
    {% elseif status == "departed" %}    
        Parti
    {% elseif status == "ouvert" %} 
        Enregistrement ouvert
    {% endif %} 
{% endblock %}